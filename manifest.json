{
  "manifest_version": 2,

  "name": "No News",
  "permissions": [
  "storage"
  ],
  "options_page":"options.html",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",
  "web_accessible_resources": [
  "seeNo.png",
  "hearNo.png",
  "sayNo.png"
  ],
  "browser_action": {
    "default_icon": "sayNo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-1.11.1.min.js", "popup.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
