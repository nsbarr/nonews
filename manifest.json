{
  "manifest_version": 2,

  "name": "No News",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-1.11.1.min.js", "popup.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
